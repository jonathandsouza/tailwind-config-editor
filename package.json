{
	"name": "tailwind-configuration-editor",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
		"prepare": "husky install"
	},
	"git": {
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*": [
			"prettier --ignore-unknown --write"
		]
	},
	"dependencies": {
		"@types/node": "20.3.1",
		"@types/react": "18.2.12",
		"@types/react-dom": "18.2.5",
		"autoprefixer": "10.4.14",
		"cssnano": "^6.0.1",
		"eslint": "8.42.0",
		"eslint-config-next": "13.4.5",
		"next": "13.4.5",
		"postcss": "8.4.24",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"swr": "^2.1.5",
		"tailwindcss": "3.3.2",
		"typescript": "5.1.3"
	},
	"devDependencies": {
		"daisyui": "^3.1.1",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8"
	}
}
